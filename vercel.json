{
  "version": 2,
  "builds": [
    { "src": "camerarental/wsgi.py", "use": "@vercel/python" },
    {"src":"build_files.sh","use":"@vercel/static-build"}
  ],
  "routes": [
    { "src": "/(.*)", "dest": "camerarental/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "yourproject.settings"
  }
}
